{
  "name": "@kaniamb/braab",
  "version": "1.0.4",
  "description": "A simple http server with state",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": "./dist/index.js",
  "dependencies": {
    "@polka/parse": "^1.0.0-next.0",
    "got": "^11.8.6",
    "polka": "^0.5.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@tsconfig/node12": "^1.0.11",
    "@types/node": "^18.11.12",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4"
  },
  "scripts": {
    "prepare": "npm run compile:all && husky install",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "compile:all": "tsc --diagnostics",
    "clean:build": "rimraf dist",
    "compile": "run-s clean:build compile:all",
    "test": "cross-env node tests/index.js",
    "clean:test": "run-s clean:pretest test",
    "compile:test": "run-s compile test"
  }
}
